[env]
time_limit = 25
time_step = 0.25
val_size = 100
test_size = 500
randomize_attributes = false


[reward]
success_reward = 1
travelling_reward_factor = 0.08
collision_penalty = -0.25
discomfort_dist = 0.2
discomfort_dist_slow = 0.2
discomfort_dist_fast = 0.3
discomfort_penalty_factor = 0.5



[sim]
train_val_sim = circle_crossing
test_sim = mixed
square_width = 10
circle_radius = 4
human_num = 5


[humans]
visible = true
policy = pysf
radius = 0.3
v_pref = 1
sensor = coordinates


[humans_slow]
visible = true
policy = pysf
radius = 0.3
v_pref = 0.5
sensor = coordinates


[humans_fast]
visible = true
policy = pysf
radius = 0.3
v_pref = 1.5
sensor = coordinates


[robot]
visible = true
policy = none
radius = 0.3
v_pref = 1
sensor = coordinates
